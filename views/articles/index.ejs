<%- include('../partials/header') %>

<div class="show-articles">
    
    <% articles.forEach(a => { %>
        <tr>
            <td class="article-headline"><%= a.headline %></td>
            <td class="article-timestamp"><%= a.articleDate.toLocaleDateString() %> <%= a.articleDate.toLocaleTimeString() %></td><br>
        </tr>

    <% }) %>
</div>

<%- include('../partials/footer') %>